{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "json": {
      "format": "{\"time\": \"%(asctime)s\", \"level\": \"%(levelname)s\", \"name\": \"%(name)s\", \"msg\": %(message)s}"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "json",
      "level": "INFO"
    },
    "file": {
      "class": "logging.FileHandler",
      "formatter": "json",
      "filename": "AICoreDirector.log",
      "encoding": "utf-8",
      "level": "INFO"
    }
  },
  "root": {
    "handlers": ["console", "file"],
    "level": "INFO"
  }
} 